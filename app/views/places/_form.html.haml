= semantic_form_for @place do |f|
  = f.input :website_id, :as => :hidden, :value => @website
  = f.error_messages
  - f.inputs do
    = f.input :website # , :as => :string, :input_html2 => { :disabled => true } 
    = f.input :name
    = f.input :geometry, :as => :select, :collection => bannerstyle.all
  = f.submit
