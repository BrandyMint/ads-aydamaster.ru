.new_session

  %h2 Вход

  - resource.remember_me = true
  = simple_form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
    = f.error_messages
    = f.input :email
    = f.input :password
    = f.input :remember_me, :as => :boolean
    = f.button :submit

= render :partial => "devise/shared/links"
